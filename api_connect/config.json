{
  "name": "API Connect-Smart",
  "version": "1.0.0",
  "slug": "api",
  "description": "API Connect-Smart",
  "url": "https://github.com/Connect-Smart/hassio/tree/main/api_connect",
  "arch": ["armv7", "armhf", "amd64", "aarch64", "i386"],
  "startup": "system",
  "init": false,
  "boot": "auto",
  "host_network": true,
  "homeassistant": "/config",
  "ingress": [
    {
      "port": 8099,
      "local_path": "/usr/share/nginx/html",
      "remote_path": "/",
      "type": "web"
    }
  ],
  "ingress_panel": true,
  "ingress_url": "/admin",
  "ingress_entry": "admin",
  "ingress_category": "config",
  "ingress_domain": "your_addon_domain",
  "options": {
    "token": "",
    "entity": "",
    "apikey": ""
  },
  "schema": {
    "token": "str",
    "entity": "str",
    "apikey": "str"
  }
}
